Cell[TextData[{
 "a) Make a 10\[Times]10 matrix A with ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubscriptBox["a", 
     RowBox[{"i", " ", "j"}]], "=", 
    RowBox[{"i", "+", "j"}]}], TraditionalForm]],ExpressionUUID->
  "bab5d7e2-aa3e-4411-a1ae-43e97738fd8d"],
 " as ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"\[LeftBracketingBar]", 
     RowBox[{"i", "-", "j"}], "\[RightBracketingBar]"}], "\[LessEqual]", 
    "1"}], TraditionalForm]],ExpressionUUID->
  "33a4d2d2-08a8-45e9-8673-522147088e14"],
