   3.56904446879274*^9, 3.5690444688027406`*^9}, {3.5690448340562625`*^9, 
   3.569044835175374*^9}, {3.66365844751855*^9, 3.6636585290993986`*^9}, {
   3.6636585816626205`*^9, 3.6636585816782646`*^9}, {3.72526234313308*^9, 
