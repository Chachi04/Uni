Cell[TextData[{
 "Determine the following integrals and verify the result by differentiating \
again:\na. ",
 Cell[BoxData[
  FormBox[
   RowBox[{"\[Integral]", 
    RowBox[{
     RowBox[{
      SuperscriptBox["x", "3"], " ", 
      RowBox[{"sin", "(", 
       SuperscriptBox["x", "2"], ")"}]}], 
     RowBox[{"\[DifferentialD]", "x"}]}]}], TraditionalForm]],ExpressionUUID->
  "7a872cd2-d8f4-47ce-9252-4752efb7aaa8"],
 "\nb. ",
 Cell[BoxData[
  FormBox[
   RowBox[{"\[Integral]", 
    RowBox[{
     RowBox[{
      SuperscriptBox["log", "10"], "(", "x", ")"}], 
     RowBox[{"\[DifferentialD]", "x"}]}]}], TraditionalForm]],ExpressionUUID->
  "0c558627-85f9-4583-9fe9-1a2e1f0bca83"],
 "\nc. ",
 Cell[BoxData[
  FormBox[
   RowBox[{"\[Integral]", 
    RowBox[{
     FractionBox["1", 
      SuperscriptBox[
       RowBox[{"(", 
