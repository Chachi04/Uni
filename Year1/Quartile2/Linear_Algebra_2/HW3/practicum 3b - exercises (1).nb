Cell[TextData[{
 "Look up with the help function: ",
 Cell[BoxData[
  StyleBox["PolynomialRemainder",
   FontWeight->"Bold"]],ExpressionUUID->
  "22e5cce9-98bd-4a16-b0ac-e2c1cde1a8ee"],
 " and ",
 Cell[BoxData[
  StyleBox["PolynomialQuotient",
   FontWeight->"Bold"]],ExpressionUUID->
  "d1b3d801-e802-4244-92df-e39a8ae527ce"],
 ". \nCompute the remainder and quotient polynomial of  ",
 Cell[BoxData[
  FormBox[
   FractionBox[
    RowBox[{"p", "(", "x", ")"}], 
    RowBox[{"q", "(", "x", ")"}]], TraditionalForm]],ExpressionUUID->
  "f15d22d7-ad16-4fb8-920c-141f0bcf00e6"],
 " for the cases a) b) and c) below. \nThink how you would do this \
calculation by hand and try to predict the degree of the result would have \
and then calculate it using ",
 StyleBox["Mathematica",
  FontSlant->"Italic"],
 ".\na. ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"p", "(", "x", ")"}], "=", 
    RowBox[{
     RowBox[{"\[ImaginaryI]", " ", 
      SuperscriptBox["x", "3"]}], "-", 
     RowBox[{"3", " ", "x"}], "+", 
     RowBox[{"2", " ", "\[ImaginaryI]"}], "+", "1"}]}], TraditionalForm]],
  ExpressionUUID->"b9014ede-a7c7-4787-ad0e-51344d53c72c"],
 ", ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"q", "(", "x", ")"}], "=", 
    RowBox[{
     RowBox[{"3", " ", "x"}], "-", "2"}]}], TraditionalForm]],ExpressionUUID->
  "246cdff9-cdd9-4f04-b281-91a550c110d1"],
 ".\nb. ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"p", "(", "x", ")"}], "=", 
    RowBox[{
     SuperscriptBox["x", "8"], "-", 
     SuperscriptBox["x", "6"], "+", "1"}]}], TraditionalForm]],ExpressionUUID->
  "f9437ed6-7150-4c04-b8a2-306b7a97f1f9"],
 ", ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"q", "(", "x", ")"}], "=", 
    RowBox[{
     SuperscriptBox["x", "2"], "+", "1"}]}], TraditionalForm]],ExpressionUUID->
  "baf3d8b7-0b1e-435d-ae22-c34683b18469"],
 ".\nc. ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"p", "(", "x", ")"}], "=", 
    RowBox[{
     SuperscriptBox["x", "5"], "+", 
     RowBox[{
      RowBox[{"(", 
       RowBox[{"\[ImaginaryI]", "+", "3"}], ")"}], " ", 
      SuperscriptBox["x", "2"]}], "+", "1"}]}], TraditionalForm]],
  ExpressionUUID->"2fd47a2d-9e7a-4ecd-bcb5-ea5583ecf76b"],
 ", ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"q", "(", "x", ")"}], "=", 
    RowBox[{
     SuperscriptBox["x", "3"], "+", "\[ImaginaryI]"}]}], TraditionalForm]],
  ExpressionUUID->"55c24161-34b9-4773-8c19-ec2d05a5d78f"],
 ".\nd. For which values of ",
 Cell[BoxData[
  FormBox["a", TraditionalForm]],ExpressionUUID->
  "e2480e42-6838-4cae-86cd-97d7e635f7b8"],
 " does the polynomial ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"p", "(", "x", ")"}], "=", 
    RowBox[{
     SuperscriptBox["x", "4"], "+", 
     RowBox[{
      RowBox[{"(", 
       RowBox[{
        SuperscriptBox["a", "2"], "-", "a", "-", "2"}], ")"}], " ", 
      SuperscriptBox["x", "3"]}], "+", 
     RowBox[{
      RowBox[{"(", 
       RowBox[{
        SuperscriptBox["a", "3"], "-", 
        RowBox[{"2", " ", 
         SuperscriptBox["a", "2"]}], "-", 
        RowBox[{"2", " ", "a"}], "+", "3"}], ")"}], " ", 
      SuperscriptBox["x", "2"]}], "+", 
     RowBox[{
      RowBox[{"(", 
       RowBox[{
        RowBox[{"2", " ", 
         SuperscriptBox["a", "2"]}], "-", 
        RowBox[{"3", " ", "a"}], "-", "4"}], ")"}], " ", "x"}], "+", "2"}]}], 
   TraditionalForm]],ExpressionUUID->"7f89335e-5531-4ff2-a8e0-d2e8c9c24fbb"],
 " contain a factor ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SuperscriptBox["x", "2"], "+", "x", "+", "1"}], TraditionalForm]],
  ExpressionUUID->"fb496288-205e-4f57-9d45-756255bdcc32"],
 "?"
}], "Text",
