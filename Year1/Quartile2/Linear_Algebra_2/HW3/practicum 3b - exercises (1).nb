Cell[TextData[{
 "a) Compute (1)",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubsuperscriptBox["\[Integral]", 
     RowBox[{"-", "\[Pi]"}], "\[Pi]"], 
    RowBox[{
     RowBox[{
      RowBox[{"cos", "(", 
       RowBox[{"a", " ", "x"}], ")"}], " ", 
      RowBox[{"cos", "(", 
       RowBox[{"b", " ", "x"}], ")"}]}], 
     RowBox[{"\[DifferentialD]", "x"}], " ", 
     RowBox[{"(", "1", ")"}], " "}]}], TraditionalForm]],ExpressionUUID->
  "c3768cab-34e3-4e2a-b2fb-0ab39247e511"],
 ", ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"(", "2", ")"}], " ", 
    RowBox[{
     SubsuperscriptBox["\[Integral]", 
      RowBox[{"-", "\[Pi]"}], "\[Pi]"], 
     RowBox[{
      RowBox[{
       RowBox[{"sin", "(", 
        RowBox[{"a", " ", "x"}], ")"}], " ", 
       RowBox[{"sin", "(", 
        RowBox[{"b", " ", "x"}], ")"}]}], 
      RowBox[{"\[DifferentialD]", "x"}]}]}]}], TraditionalForm]],
  ExpressionUUID->"97d3c3c8-d1cc-4f49-a31c-c8cfedacf8c7"],
 " and (3) ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubsuperscriptBox["\[Integral]", 
     RowBox[{"-", "\[Pi]"}], "\[Pi]"], 
    RowBox[{
     RowBox[{
      RowBox[{"sin", "(", 
       RowBox[{"a", " ", "x"}], ")"}], " ", 
      RowBox[{"cos", "(", 
       RowBox[{"b", " ", "x"}], ")"}]}], 
     RowBox[{"\[DifferentialD]", "x"}]}]}], TraditionalForm]],ExpressionUUID->
  "edc3f668-89d6-4027-a38f-dc1170acb2d1"],
 ". Here ",
 Cell[BoxData[
  FormBox["a", TraditionalForm]],ExpressionUUID->
  "8e2d29b8-5cac-4d50-9b14-8b8b2afd4ce5"],
 " and ",
 Cell[BoxData[
  FormBox["b", TraditionalForm]],ExpressionUUID->
  "f94a1792-c471-4a38-acab-79bf73fff63c"],
 " are real numbers. \nb) What are the results if ",
 Cell[BoxData[
  FormBox["a", TraditionalForm]],ExpressionUUID->
  "94a50ab5-2d58-4672-99f5-0d5ad5b43079"],
 " and ",
 Cell[BoxData[
  FormBox["b", TraditionalForm]],ExpressionUUID->
  "e5ef4f9f-ec79-4e07-9983-a9c06e72ceeb"],
 " are integers? \nc) What are the results if ",
 Cell[BoxData[
  FormBox["a", TraditionalForm]],ExpressionUUID->
  "a36c7fb6-a564-4c1d-9e4c-9a9052ff5ee7"],
 " and ",
 Cell[BoxData[
  FormBox["b", TraditionalForm]],ExpressionUUID->
  "505ca6ac-d4f8-44d2-9581-0368c062a34a"],
 " are the same integer?\n\nd) Compute ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"(", "1", ")"}], " ", 
    RowBox[{"\[Integral]", 
     RowBox[{
      SuperscriptBox["\[ExponentialE]", 
       RowBox[{
        RowBox[{"-", "a"}], " ", "x"}]], " ", 
      RowBox[{"sin", "(", 
       RowBox[{"b", " ", "x"}], ")"}], 
      RowBox[{"\[DifferentialD]", "x"}]}]}]}], TraditionalForm]],
  ExpressionUUID->"71689441-7f2b-456a-91ba-313a011770f9"],
 " and (2) ",
 Cell[BoxData[
  FormBox[
   RowBox[{"\[Integral]", 
    RowBox[{
     RowBox[{
      SuperscriptBox["\[ExponentialE]", 
       RowBox[{
        RowBox[{"-", "a"}], " ", "x"}]], " ", 
      RowBox[{"cos", "(", 
       RowBox[{"b", " ", "x"}], ")"}]}], 
     RowBox[{"\[DifferentialD]", "x"}]}]}], TraditionalForm]],ExpressionUUID->
  "35bff3ce-9024-4943-8dd6-7d85d9c8a49d"],
 ". Here ",
 Cell[BoxData[
  FormBox["a", TraditionalForm]],ExpressionUUID->
  "0ed20583-1405-481c-b725-f8d17542858c"],
 " and ",
 Cell[BoxData[
  FormBox["b", TraditionalForm]],ExpressionUUID->
  "ea9ccf20-a148-4ea0-8f1e-d4054e461ae6"],
 " are real numbers with ",
 Cell[BoxData[
  FormBox[
   RowBox[{"a", ">", "0"}], TraditionalForm]],ExpressionUUID->
  "f70d598f-904f-41dd-b5f5-f004e31733be"],
