    RowBox[{"12", " ", "y", " ", "z"}]}], TraditionalForm]],ExpressionUUID->
  "50d918e3-7995-4f9a-9241-4e41aab68df8"],
 "\n\nb) ",
 Cell[BoxData[
  FormBox[
   StyleBox[
    RowBox[{
     RowBox[{"-", 
      FractionBox[
       RowBox[{
        RowBox[{"(", 
         RowBox[{
          RowBox[{"3", " ", "a"}], "-", 
          RowBox[{"5", " ", "c"}]}], ")"}], " ", 
        SuperscriptBox["c", "2"]}], 
       RowBox[{
        RowBox[{"a", " ", "b", " ", "c"}], "-", 
        RowBox[{
         SuperscriptBox["a", "2"], " ", "b"}]}]]}], "+", 
     RowBox[{"b", " ", 
      RowBox[{"(", 
       RowBox[{
        FractionBox[
         RowBox[{
          RowBox[{"(", 
           RowBox[{
            RowBox[{"3", " ", "a"}], "-", 
            RowBox[{"5", " ", "c"}]}], ")"}], " ", 
          SuperscriptBox["c", "2"]}], 
         RowBox[{"a", " ", 
          SuperscriptBox["b", "2"], " ", 
          RowBox[{"(", 
           RowBox[{"c", "-", "a"}], ")"}]}]], "+", 
        FractionBox["2", "b"]}], ")"}]}], "-", "2"}], "DisplayFormula"], 
   TraditionalForm]],ExpressionUUID->"b674c228-3c64-43d6-8fbc-e1b486d49ad2"],
