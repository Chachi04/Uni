  FormBox[
   RowBox[{
    SuperscriptBox["x", "2"], "+", 
    RowBox[{"4", " ", "y", " ", "x"}], "+", 
    RowBox[{"6", " ", "z", " ", "x"}], "+", 
    RowBox[{"4", " ", 
